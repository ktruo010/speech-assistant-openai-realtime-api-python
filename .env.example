OPENAI_API_KEY=your_openai_api_key

# Language configuration (optional - defaults to Vietnamese)
# Options: 'vi' for Vietnamese, 'en' for English
LANGUAGE=vi

# Maximum call duration in seconds (optional - defaults to 3600 seconds / 1 hour)
# Set to 0 to disable call duration limit
# Examples: 3600 = 1 hour, 1800 = 30 minutes, 7200 = 2 hours
MAX_CALL_DURATION=3600

# Google Custom Search API credentials (optional - for web search)
# Get your API key from: https://console.cloud.google.com/apis/credentials
# Get your CSE ID from: https://programmablesearchengine.google.com/
GOOGLE_API_KEY=your_google_api_key
GOOGLE_CSE_ID=your_google_cse_id